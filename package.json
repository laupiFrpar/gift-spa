    "pinia": "^2.0.11",
    "vite": "^2.8.4",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@fortawesome/fontawesome-free": "^6.0.0",
