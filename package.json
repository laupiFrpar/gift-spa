    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "core-js": "^3.21.1",
    "cypress": "^9.6.0",
    "eslint": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
